    "bcryptjs": "^2.4.3",
    "client": "file:client",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-form-data": "^2.0.19",
    "express-validator": "^6.8.1",
    "gravatar": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.8",
    "multer": "^1.4.5-lts.1",
    "normalize-url": "^5.3.0",
    "postcode-validator": "^3.7.0",
    "validate-phone-number-node-js": "^0.0.1"
